{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sveltekit-project-creator",
  "title": "SvelteKit Project Creator",
  "description": "Creates SvelteKit Projects",
  "icon": "command-icon.png",
  "author": "takoyaro",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "New SvelteKit Project",
      "description": "Creates a new SvelteKit project in your default SvelteKit projects path",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.7"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}